#wp{background-color: #f2e9bd;}
/*轮播图片*/
#wp .scroll { position: relative;overflow: hidden;z-index:-9999;}
#wp .scroll #slider li { position: relative; overflow: hidden; }
#wp .scroll .pp { position: absolute; width: 100%; text-align: center; bottom: 0px; }
#wp .scroll #position { }
#wp .scroll #position em { box-shadow: 0px 0px 1px #A2A5B6; display: inline-block; width: 10px; height: 10px; margin: 0px 2px; text-indent: -9999px; border-radius: 6px; overflow: hidden;  }
#wp .scroll #position em.on { background-color: #EFEFEF; }
#wp .scroll a#prev, .scroll a#next { height: 34px; width: 34px; color: #444; text-decoration: none; position: absolute; top: 50%; z-index: 1; margin-top: -17px; }
#wp .scroll a#next:after { right: 15px; -webkit-transform: rotate(-45deg); -moz-transform: rotate(-45deg); -o-transform: rotate(-45deg); -ms-transform: rotate(-45deg); transform: rotate(-45deg); }
#wp .scroll a#prev:after, .scroll a#next:after { content: ''; top: 37%; position: absolute; width: 7px; height: 7px; border-color: #fff; border-style: solid; border-width: 0 2px 2px 0; }
#wp .scroll a#prev:after { left: 15px; -webkit-transform: rotate(135deg); -moz-transform: rotate(135deg); -o-transform: rotate(135deg); -ms-transform: rotate(135deg); transform: rotate(135deg); }
#wp .scroll a#prev { left: 0px; }
#wp .scroll a#next { right: 0px; }
#wp .scroll .stbg { height: 15px; width: 100%; color: #fff; text-indent: 10px; position: absolute; z-index: 1; background: url(/source/plugin/polls/static/images/stbg.png) repeat 0 0; padding-top: 10px; padding-right: 0px; padding-bottom: 10px; padding-left: 0px; bottom: 0px; }
#wp .scroll #slider img { width: 100%; vertical-align: top; }  /*vertical-align:top;   解决A标签自带5像素问题*/
#wp .s_1 a#prev, #wp .s_1 a#next, #wp .s_1 .stbg, #wp .s_1 #position { display: none; }
